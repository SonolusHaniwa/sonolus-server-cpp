{
    "levels.enableLike": true,
    "levels.enableComment": true,
    "levels.enableRating": true,
    "levels.enableGUICreate": false,
    "levels.enableSonolusCreate": false,
    "levels.exceptGUICreate": [],
    "levels.exceptSonolusCreate": [],
    "levels.pageSize.info": 5,
    "levels.pageSize.list": 20,
    "levels.pageSize.recommends": 5,
    "levels.pageSize.community": 5,
    "levels.info.sections": [
        {
            "title": "#NEWEST",
            "icon": "level",
            "filter": "",
            "order": "id DESC"
        }
    ],
    "levels.details.sections": [
        {
            "title": "#RECOMMENDED",
            "icon": "star",
            "filter": "title = \"{{title}}\"",
            "order": "id DESC"
        }
    ],
    "levels.searches": [
        {
            "title": "#ADVANCED",
            "icon": "search",
            "type": "advanced",
            "options": [
                {
                    "query": "keywords",
                    "name": "#KEYWORDS",
                    "type": "text",
                    "placeholder": "#KEYWORDS_PLACEHOLDER"
                },
                {
                    "query": "artist",
                    "name": "#ARTISTS",
                    "type": "text",
                    "placeholder": "#ARTISTS_PLACEHOLDER"
                },
                {
                    "query": "author",
                    "name": "#AUTHOR",
                    "type": "text",
                    "placeholder": "#AUTHOR_PLACEHOLDER"
                },
                {
                    "query": "minRating",
                    "name": "#RATING_MINIMUM",
                    "type": "slider",
                    "def": 1,
                    "min": 1,
                    "max": 50,
                    "step": 1
                },
                {
                    "query": "maxRating",
                    "name": "#RATING_MAXIMUM",
                    "type": "slider",
                    "def": 40,
                    "min": 1,
                    "max": 50,
                    "step": 1
                },
                {
                    "query": "random",
                    "name": "#RANDOM",
                    "type": "toggle",
                    "def": 0
                }
            ],
            "filter": "title LIKE \"%{{keywords}}%\" AND artists LIKE \"%{{artist}}%\" AND author LIKE \"%{{author}}%\" AND rating >= {{minRating}} AND rating <= {{maxRating}}",
            "order": "CASE {{random}} WHEN 0 THEN id WHEN 1 THEN RANDOM() END DESC"
        }
    ],
    "levels.creates": [
        {
            "title": "#ADVANCED",
            "icon": "plus",
            "type": "advanced",
            "options": [
                {
                    "query": "name",
                    "name": "#NAME",
                    "type": "text",
                    "placeholder": "#NAME_PLACEHOLDER",
                    "required": true
                },
                {
                    "query": "rating",
                    "name": "#RATING",
                    "type": "slider",
                    "def": 5,
                    "min": 5,
                    "max": 50,
                    "step": 1
                },
                {
                    "query": "title",
                    "name": "#TITLE",
                    "type": "text",
                    "placeholder": "#TITLE_PLACEHOLDER",
                    "required": true
                },
                {
                    "query": "artists",
                    "name": "#ARTISTS",
                    "type": "text",
                    "placeholder": "#ARTISTS_PLACEHOLDER"
                },
                {
                    "query": "author",
                    "name": "#AUTHOR",
                    "type": "text",
                    "placeholder": "#AUTHOR_PLACEHOLDER"
                },
                {
                    "query": "description",
                    "name": "Description",
                    "type": "textArea",
                    "placeholder": "Enter description..."
                },
                {
                    "query": "tags",
                    "name": "Tags",
                    "type": "text",
                    "placeholder": "Enter tags..."
                },
                {
                    "query": "localization",
                    "name": "Localization",
                    "type": "localizationItem"
                },
                {
                    "query": "engine",
                    "name": "Engine",
                    "type": "serverItem",
                    "itemType": "engine",
                    "required": true
                },
                {
                    "query": "skin",
                    "name": "Skin",
                    "type": "serverItem",
                    "itemType": "skin"
                },
                {
                    "query": "background",
                    "name": "Background",
                    "type": "serverItem",
                    "itemType": "background"
                },
                {
                    "query": "effect",
                    "name": "Effect",
                    "type": "serverItem",
                    "itemType": "effect"
                },
                {
                    "query": "particle",
                    "name": "Particle",
                    "type": "serverItem",
                    "itemType": "particle"
                },
                {
                    "query": "cover",
                    "name": "Cover",
                    "type": "file",
                    "required": true
                },
                {
                    "query": "bgm",
                    "name": "BGM",
                    "type": "file",
                    "required": true
                },
                {
                    "query": "data",
                    "name": "Data",
                    "type": "file",
                    "required": true
                },
                {
                    "query": "preview",
                    "name": "Preview",
                    "type": "file",
                    "required": true
                }
            ],
            "values": {
                "name": "{{name}}",
                "rating": "{{rating}}",
                "title": "{{title}}",
                "artists": "{{artists}}",
                "author": "{{author}}",
